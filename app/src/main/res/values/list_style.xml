<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="listDates" parent="TextAppearance.AppCompat.Headline">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#1d0e4d</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">30dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:typeface">monospace</item>
    </style>
    <style name="questions" parent="TextAppearance.AppCompat.Large">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">#1d0e4d</item>
    </style>
    <style name="answers" parent="TextAppearance.AppCompat.Medium">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">#4d0e0e</item>
    </style>
</resources>